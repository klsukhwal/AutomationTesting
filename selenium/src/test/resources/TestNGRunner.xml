<?xml version="1.0" encoding="UTF-8"?>

<suite name="Test Suite for Login and Logout from Demo app" verbose="1">
	<test name="TestCycle1">
		<classes>
			<class name="seleniumphase2.testannotation">
				<methods>
					<include name="LoginWithValidCredentials"></include>
				</methods>
			</class>
			<class name="seleniumphase2.BeforeAfterMethodAnnotation">
				<methods>
					<exclude name="logoutFromApplication"></exclude>
				</methods>
			</class>
		</classes>
	</test>
</suite>